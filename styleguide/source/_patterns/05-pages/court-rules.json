      {
        "compHeading": {
          "title": "(a) Civil Cases",
          "sub": false,
          "color": "",
          "id": "civil-cases",
          "centered": ""
        },
        "rteElements": [
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "Magistrates may hear and rule on the following motions in civil actions provided that such motions are uncontested or assented to in writing and provided further that neither parry is proceeding pro se:"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(1) For Entry of Default"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(2) For Entry of Default Judgment under Mass. R. Civ. P. 55(b)(1) or Dist./Mun. Cts. R. Civ. P., 55(b)(1)"
              }
            }
          }, {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(3) To Amend any Pleading"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(4) To File a Responsive Pleading Late"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(5) For Order of Notice"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(6) To Strike Out and Answer Over Interrogatories"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(7) To Compel Answers to Interrogatories"
              }
            }

          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(8) For Consolidation of Cases Pending in One Court"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(9) To Remove Default Entered by a Magistrate"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(10) To Permit Marriage Without Delay (Probate and Family Court Department)"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(11) To Waive the Thirty-Days Living Apart Requirement (Probate and Family Court Department)"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "(12) To Extend or Shorten Time or Continue a Hearing on a Motion"
              }
            }
          },
          {
            "path": "@atoms/11-text/paragraph.twig",
            "data": {
              "paragraph": {
                "text": "Within a Department of the Trial Court, the Administrative Justice of the Department may delete or make additions to the motions enumerated in this section."
              }
            }
          }
        ]
      },
